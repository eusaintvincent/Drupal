<div class="row">
    <div class="col-md-8">
        <div class="row">
            <div class="col-md-4">
                <img src="{{ file_url(node.image.value) }}" alt="{{ node.image.alt }}"/>
            </div>
            <div class="col-md-8">
                {{ kint(_context|keys) }}
                <div>{{ node.title.value }}</div>
                <div><span>{{ node.date.value|date('d/m/y') }} | {#{{ node.utilisateur.0.entity.user.nom_editeur }}#} | {{ node.secondary_category.value }}</span></div>
                <div>{{ node.main_category.value }}</div>
                <div>{{ node.resume.value }}</div>
            </div>
        </div>
    </div>
</div>